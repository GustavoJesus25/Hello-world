package main

import (
	"fmt"
)

var y = "Bom dia"

func main() { //aqui começa

	x := 10

	fmt.Printf("x: %v, %T\n", x, x)
	fmt.Printf("y: %v, %T\n\n", y, y)
	// z só pode ser mudado depois de sua declaração
	x, z := 20, 30
	fmt.Printf("x: %v, %T\n", x, x)
	fmt.Printf("z: %v, %T\n", z, z)
} //aqui termina
